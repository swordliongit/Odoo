<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <!-- Feedback Form View -->
  <record id="feedback_form" model="ir.ui.view">
    <field name="name">Feedback</field>
    <field name="model">feedback.profile</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <group>
              <field name="name"/>
              <field name="contact_id"/>
            </group>
          </group>
        </sheet>
      </form>
    </field>
  </record>
  <!--Feedback Tree View-->
  <record id="feedback_tree" model="ir.ui.view">
    <field name="name">Feedback</field>
    <field name="model">feedback.profile</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
        <field name="contact_id"/>
      </tree>
    </field>
  </record>
  <!-- Action to call the views-->
  <record id="feedback_action" model="ir.actions.act_window">
    <field name="name">Feedback</field>
    <field name="type">ir.actions.act_window</field>
    <field name="res_model">feedback.profile</field>
    <field name="view_mode">tree,form</field>
  </record>
  <!-- Feedback Menu-->
  <menuitem id="feedback_menu" name="Feedback" sequence="4" action="feedback_action"/>
</odoo>